# # Create a function called greet(). 

# def greet():
#   print("Hello {world}")
#   print("How do you do?")
#   print("Isn't the weather nice today?")

# greet()


def greet_with_name(name):
  print(f"Hello {name}")
  print(f"How do you do {name}?")
  print(f"Isn't the weather nice today {name}?")

greet_with_name("Matt")

